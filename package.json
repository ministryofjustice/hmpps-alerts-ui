{
  "name": "hmpps-alerts-ui",
  "version": "0.0.1",
  "description": "HMPPS Alerts UI",
  "repository": "git@github.com:ministryofjustice/hmpps-alerts-ui.git",
  "license": "MIT",
  "scripts": {
    "prepare": "hmpps-precommit-hooks-prepare",
    "watch-ts": "tsc -w",
    "build": "node esbuild/esbuild",
    "start": "node $NODE_OPTIONS dist/server.js | bunyan -o short",
    "start:dev": "node esbuild/esbuild --watch",
    "start-feature": "nyc instrument dist --nycrc-path ./.nycrc && node $NODE_DEBUG_OPTION --env-file=feature.env dist/server.js | bunyan -o short",
    "start-feature:dev": "node esbuild/esbuild --watch --env feature.env",
    "lint": "eslint . --cache --max-warnings 0",
    "lint-fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc && tsc -p integration_tests && tsc -p server/routes",
    "test": "jest",
    "test:ci": "jest",
    "security_audit": "audit-ci --config audit-ci.json",
    "int-test": "cypress run --config video=false",
    "int-test-ui": "cypress open --e2e --browser chrome",
    "clean": "rm -rf dist test_results coverage",
    "rebuild": "npm run clean && npm i && npm run build",
    "precommit:secrets": "gitleaks git --pre-commit --redact --staged --verbose --config .gitleaks/config.toml --gitleaks-ignore-path .gitleaks/.gitleaksignore",
    "precommit:lint": "lint-staged",
    "precommit:verify": "npm run typecheck && npm test",
    "setup": "npm ci && hmpps-npm-script-run-allowlist"
  },
  "engines": {
    "node": "^24",
    "npm": "^11"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.928.0",
    "@cypress/code-coverage": "^3.14.7",
    "@ministryofjustice/frontend": "^7.1.0",
    "@ministryofjustice/hmpps-auth-clients": "^1.0.0",
    "@ministryofjustice/hmpps-connect-dps-components": "^5.2.0",
    "@ministryofjustice/hmpps-monitoring": "^1.0.0",
    "@ministryofjustice/hmpps-npm-script-allowlist": "0.0.3",
    "@ministryofjustice/hmpps-rest-client": "^1.0.0",
    "@sentry/cli": "^2.58.0",
    "@sentry/node": "^10.24.0",
    "agentkeepalive": "^4.6.0",
    "applicationinsights": "^2.9.8",
    "body-parser": "^2.2.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.8.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^9.0.0",
    "csrf-sync": "^4.2.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "govuk-frontend": "^5.13.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "jquery": "^3.7.1",
    "jwt-decode": "^4.0.0",
    "multer": "^2.0.2",
    "nocache": "^4.0.0",
    "nunjucks": "^3.2.4",
    "openapi-typescript": "^7.10.1",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "prom-client": "^15.1.3",
    "redis": "^5.9.0",
    "superagent": "^10.2.3",
    "url-value-parser": "^2.2.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jgoz/esbuild-plugin-typecheck": "^4.0.4",
    "@ministryofjustice/eslint-config-hmpps": "^1.0.1",
    "@ministryofjustice/hmpps-precommit-hooks": "^1.0.2",
    "@testing-library/cypress": "^10.1.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/bunyan": "^1.8.11",
    "@types/bunyan-format": "^0.2.9",
    "@types/compression": "^1.8.1",
    "@types/connect-flash": "0.0.40",
    "@types/cypress__code-coverage": "^3.10.2",
    "@types/express-session": "^1.18.2",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.17",
    "@types/passport-oauth2": "^1.8.0",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "audit-ci": "^7.1.0",
    "aws-sdk-client-mock": "^4.1.0",
    "axe-core": "^4.11.0",
    "cypress": "^15.6.0",
    "cypress-axe": "^1.7.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^2.0.5",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-manifest": "^1.0.5",
    "esbuild-sass-plugin": "^3.3.1",
    "jest": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.2.6",
    "mocha-junit-reporter": "^2.2.1",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jinja-template": "^2.1.0",
    "sass": "^1.94.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3"
  }
}
